			<limit effort="30" lower="-2.87979" upper="2.87979" velocity="4.36332"/>
			<axis xyz="0 0 1"/>
			<dynamics damping="0.5" friction="0.0"/>
		</joint>
		<joint name="${prefix}joint_2" type="revolute">
